(program
  (import TestBase)

  (structure Click)
  (external (: click Click))

  (structure name (: x Int) (: y Bool))

  (: std_test (List Int))
  (= std_test (initnext (list 0 1) (push! std_test 2)))

  (: randint Int)
  (= randint (initnext 0 (uniformChoice std_test)))

  (: arange (List Int))
  (= arange (range 1 5))
)