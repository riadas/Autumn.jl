(program
  (import TestBase)

  (structure Click)
  (external (: click Click))

  (structure Thing (: x Int) (: y Bool))
  (: thing Thing)
  (= thing (initnext (Thing 0 false) (Thing (- 1 (.. (prev thing) x)) (! (.. (prev thing) y)))))

  (: std_test (List Int))
  (= std_test (initnext (list 0 1) (push! std_test 2)))

  (: randint Int)
  (= randint (initnext 0 (uniformChoice std_test)))

  (: arange (List Int))
  (= arange (range 1 5))
)